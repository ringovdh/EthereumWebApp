<?xml version='1.0' encoding='UTF-8'?>
<Context>
	<Resource 
		name="jdbc/healthCheck" 
		type="javax.sql.DataSource"
		driverClassName="net.sourceforge.jtds.jdbc.Driver" 
		url="jdbc:jtds:mysql://localhost:3306/healthCheck"
